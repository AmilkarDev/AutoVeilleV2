@model  Autoveille.Models.Appels
@{
    ViewBag.Title = "InfoConcession";
}

<div class="alert alert-dark" style="height:60px;font-weight:bold;background-color:black">
    <p style="float:left;color:white;font-weight:bold;">NOM CONCESSIONNAIRE</p>
    <p style="float:right;color:white;font-weight:bold;">12 AVRIL AU 15 AVRIL 2021</p>
</div>
<div class="alert alert-dark row" style="height:60px;font-weight:bold;background-color:white;margin-top:-15px;">
    <p class="col-md-4" style="float: left; color: black; font-weight: bold;">PROSPECTS TOTAL</p>
    <p class="col-md-4" style="text-align:center;color:black;font-weight:bold;">PROSPECTS REJOINTS</p>
    <p class="col-md-4 pull-right" style="text-align:right;float: right; color: black; font-weight: bold;">PROSPECTS A REJOINDRE</p>
</div>


<div id="dvCampaignDetails" style="width:75%">

</div>
<div id="dvClientDetails" style="width:25%">

</div>



<script type="text/javascript">
    $(document).ready(function () {
        $.ajax({
            url: '/Home/GetNomAppels',
            contentType: 'application/html; charset=utf-8',
            type: 'GET',
            dataType: 'html',
            success: function (result) {
                $('#dvCampaignDetails').html(result);
            },
            error: function (xhr, status) {
                alert(status);
            }
        });
    });
</script>